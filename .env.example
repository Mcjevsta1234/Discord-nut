# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here

# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# OpenRouter Models
# You can use different models for different purposes
# See https://openrouter.ai/models for available models
OPENROUTER_MODEL_ROUTER=openai/gpt-3.5-turbo
OPENROUTER_MODEL_CHAT=openai/gpt-3.5-turbo
OPENROUTER_MODEL_SUMMARIZER=openai/gpt-3.5-turbo

# Bot Configuration
BOT_SYSTEM_PROMPT=You are a helpful and friendly Discord bot assistant. Be conversational and engaging.
BOT_PERSONALITY=friendly, helpful, and slightly humorous
BOT_MAX_MEMORY_MESSAGES=10
BOT_ENABLE_SUMMARY=true
